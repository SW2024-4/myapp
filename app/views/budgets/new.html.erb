<%= form_with(model: @budget, local: true) do |f| %>
  <div class="time">
    <%= f.label :date %>
    <%= f.date_field :date %>
  </div>
  
  <div class="expend">
    <%= f.label :expend %>
    <%= f.text_field :expend %>
  </div>
  
  <div class="income">
    <%= f.label :income %>
    <%= f.text_field :income %>
  </div>
  
  <div class="submit">
    <%= f.submit %>
  </div>
  
<% end %>
