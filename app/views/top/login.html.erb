<%= form_with url: '/top/login', method: :post, local: true do |f| %>
    <%= f.label :uid, 'ユーザーID' %>
    <%= f.text_field :uid %>
    <%= f.label :pass, 'パスワード' %>
    <%= f.text_field :pass %>
    <%= f.submit 'ログイン' %>
<% end %>
<p><%= link_to 'ユーザー登録', new_user_path %></p>
<p><%= link_to 'ユーザー一覧', users_path %></p>